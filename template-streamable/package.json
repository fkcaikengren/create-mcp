{
  "name": "my-mcp",
  "version": "0.0.1",
  "description": "My MCP server",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "my-mcp": "dist/main.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "packageManager": "pnpm@10.6.2",
  "scripts": {
    "dev": "tsc -w -p tsconfig.json",
    "inspect": "npx -y mcp-inspector node dist/main.js",
    "build": "tsc -p tsconfig.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "MCP Server",
    "mcp",
    "model-context-protocol"
  ],
  "author": "Gavin Tang",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.17.2",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
